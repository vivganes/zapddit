    "@nostr-dev-kit/ndk-cache-dexie": "^2.0.3",
    "@types/linkifyjs": "^2.1.4",
    "angular-mentions": "^1.5.0",
    "bech32": "^2.0.0",
    "dexie": "^3.2.3",
    "light-bolt11-decoder": "^3.0.0",
    "linkify-html": "^4.1.1",
    "linkify-plugin-hashtag": "^4.1.1",
    "linkifyjs": "^4.1.1",
    "moment": "^2.29.4",
    "ng-in-viewport": "^16.0.0",
    "ngx-dynamic-hooks": "^2.1.0",
